local selectedDelay = 1
local autoAnswering = false
local autoAnswerLoop = nil

local delayList = {}
for i = 1, 22 do
    table.insert(delayList, tostring(i))
end

local function sendAnswer(answer)
    local ReplicaSignal = game:GetService("ReplicatedStorage"):WaitForChild("ReplicaRemoteEvents"):WaitForChild("Replica_ReplicaSignal")
    local args = {
        [1] = 2,
        [2] = "Answer",
        [3] = answer
    }
    ReplicaSignal:FireServer(unpack(args))
    print("Sended:", answer)
end
local function sendAllAnswers()
    for _, answer in ipairs(answers) do
        sendAnswer(answer)
    end
end
local function getRandomDelay()
    return math.random(6, 15)
end

local function startAutoAnswer(useRandomDelay)
    autoAnswerLoop = coroutine.create(function()
        while autoAnswering do
            sendAllAnswers()           
            local currentDelay = useRandomDelay and getRandomDelay() or selectedDelay
            wait(currentDelay)
        end
    end)
    coroutine.resume(autoAnswerLoop)
end
MainSection:CreateButton("Answer Question", function()
    sendAllAnswers()
end)

MainSection:CreateDropdown("Select Delay", delayList, 1, function(option)
    selectedDelay = tonumber(option)
end)

MainSection:CreateToggle("Auto Answer (Fixed)", function(state)
    autoAnswering = state
    if state then
        startAutoAnswer(false)
    else
        autoAnswering = false
    end
end)

MainSection:CreateToggle("Auto Answer (Random)", function(state)
    autoAnswering = state
    if state then
        startAutoAnswer(true)
    else
        autoAnswering = false
    end
end)

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "CustomNotification"
ScreenGui.Parent = game.CoreGui

-- frame
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 300, 0, 50)
Frame.Position = UDim2.new(1, -320, 1, -100)
Frame.AnchorPoint = Vector2.new(1, 1)
Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame.BackgroundTransparency = 0.2
Frame.Parent = ScreenGui

-- notification
local TextLabel = Instance.new("TextLabel")
TextLabel.Size = UDim2.new(1, -50, 1, 0)
TextLabel.Position = UDim2.new(0, 50, 0, 0)
